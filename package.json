{
  "name": "azure-blade-analytics-dashboard",
  "version": "1.0.0",
  "description": "Interactive analytics dashboard for Azure Portal blade telemetry data, providing actionable insights into user behavior and engagement patterns.",
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8000",
    "serve": "npx serve . -p 8000",
    "dev": "npx live-server --port=8000 --open=/index.html",
    "validate": "node scripts/validate-data.js",
    "build": "npm run validate && npm run minify",
    "minify": "npm run minify:css && npm run minify:js",
    "minify:css": "npx clean-css-cli -o dashboard.min.css dashboard.css",
    "minify:js": "npx terser dashboard.js -c -m -o dashboard.min.js",
    "test": "npm run test:data && npm run test:accessibility",
    "test:data": "node scripts/test-data-structure.js",
    "test:accessibility": "npx lighthouse http://localhost:8000 --only-categories=accessibility --chrome-flags='--headless'",
    "lighthouse": "npx lighthouse http://localhost:8000 --output-path=./reports/lighthouse-report.html --view",
    "deploy:azure": "az staticwebapp create --name azure-blade-analytics --resource-group your-rg --source https://github.com/johchoi-ms/Hackathon2025_CMX_Qaunt_AI_Agent --location 'Central US'",
    "deploy:netlify": "npx netlify-cli deploy --prod --dir=.",
    "deploy:vercel": "npx vercel --prod"
  },
  "keywords": [
    "azure",
    "analytics",
    "dashboard",
    "d3js",
    "data-visualization",
    "telemetry",
    "blade-usage",
    "business-intelligence",
    "azure-portal"
  ],
  "author": {
    "name": "Azure Portal Analytics Team",
    "email": "analytics@yourcompany.com"
  },
  "contributors": [
    "Amit Yanovich Barash",
    "Can Zhao", 
    "John Choi",
    "Micheal Frederick",
    "Yev Diachek"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/johchoi-ms/Hackathon2025_CMX_Qaunt_AI_Agent.git"
  },
  "bugs": {
    "url": "https://github.com/johchoi-ms/Hackathon2025_CMX_Qaunt_AI_Agent/issues"
  },
  "homepage": "https://johchoi-ms.github.io/Hackathon2025_CMX_Qaunt_AI_Agent/",
  "devDependencies": {
    "clean-css-cli": "^5.6.2",
    "lighthouse": "^11.4.0",
    "live-server": "^1.2.2",
    "netlify-cli": "^17.0.0",
    "serve": "^14.2.1",
    "terser": "^5.26.0",
    "vercel": "^32.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "files": [
    "index.html",
    "dashboard.html", 
    "dashboard.css",
    "dashboard.js",
    "test_ver11.json",
    "staticwebapp.config.json",
    "README.md",
    "USER_GUIDE.md",
    "DEPLOYMENT.md"
  ],
  "config": {
    "port": 8000
  }
}